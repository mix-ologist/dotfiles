alias lon='(keylightctl switch -timeout 15s -brightness=100 -temperature 344 --light 192.168.1.6:9123 on & keylightctl switch -timeout 15s -brightness=80 -temperature 344 --light 192.168.1.8:9123 on &)'
alias loff='(keylightctl switch -timeout 15s -brightness=100 -temperature 344 --light 192.168.1.6:9123 off & keylightctl switch -timeout 15s -brightness=80 -temperature 344 --light 192.168.1.8:9123 off &)'
